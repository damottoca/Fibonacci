object FibonacciComUmaVariavel {
        @JvmStatic
        fun main(args: Array<String>) {
            val n = 10 
            var f = 0 
            
            var i = 0
            var a = 0
            var b = 1
            while (i < n) {
                print("$a ")
                f = a + b 
                a = b 
                b = f 
                i++
            }
        }
    }
